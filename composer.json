{
  "name": "devgiants/filesystem-gpio",
  "type": "library",
  "description": "Generic library highly customizable for handling GPIO via Linux filesystem",
  "keywords": ["php", "gpio", "filesystem", "reactphp"],
  "homepage": "https://github.com/devgiants/filesystem-gpio",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas BONNIOT",
      "email": "nicolas@devgiants.fr"
    }
  ],
  "require": {
    "php": "^7.4",
    "devgiants/filesystem-notifications": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Devgiants\\FilesystemGPIO\\": "src/App"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Devgiants\\FilesystemGPIO\\Tests\\": "tests"
    }
  }
}
